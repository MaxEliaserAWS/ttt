<?xml version="1.0" encoding="utf-8"?>
<config version="1" xmlns="http://skynav.com/ns/ttv">
<option name="add-creation-metadata">false</option>
<option name="default-alignment">中頭</option>
<option name="default-kerning">1</option>
<option name="default-language">ja</option>
<option name="default-placement">横下</option>
<option name="default-region">横下</option>
<option name="default-shear">0</option>
<option name="default-whitespace">preserve</option>
<option name="merge-styles">true</option>
<option name="model">ttml2</option>
<option name="output-disable">true</option>
<option name="output-indent">false</option>
<option name="style-id-pattern">s{0}</option>
<option name="style-id-sequence-start">1</option>
<tt xmlns="http://www.w3.org/ns/ttml" xmlns:tts="http://www.w3.org/ns/ttml#styling" xmlns:ttp="http://www.w3.org/ns/ttml#parameter" ttp:profile="http://www.w3.org/ns/ttml/profile/ttml2-presentation">
<head>
<styling>
<initial tts:fontSize="4rh"/>
<initial tts:lineHeight="5rh"/>
<initial tts:showBackground="whenActive"/>
</styling>
<layout>
<region xml:id="横下" tts:writingMode="lrtb" tts:extent="80rw 15rh" tts:position="center bottom 10rh" tts:displayAlign="after"/>
<region xml:id="横上" tts:writingMode="lrtb" tts:extent="80rw 15rh" tts:position="center top 10rh" tts:displayAlign="before"/>
<region xml:id="横適" tts:writingMode="lrtb" tts:extent="80rw 80rh" tts:position="center" tts:displayAlign="justify"/>
<region xml:id="横中" tts:writingMode="lrtb" tts:extent="80rw 15rh" tts:position="center" tts:displayAlign="center"/>
<region xml:id="縦右" tts:writingMode="tbrl" tts:extent="15rw 80rh" tts:position="right 10rw center" tts:displayAlign="before"/>
<region xml:id="縦左" tts:writingMode="tbrl" tts:extent="15rw 80rh" tts:position="left 10rw center" tts:displayAlign="after"/>
<region xml:id="縦適" tts:writingMode="tbrl" tts:extent="80rw 80rh" tts:position="center" tts:displayAlign="justify"/>
<region xml:id="縦中" tts:writingMode="tbrl" tts:extent="15rw 80rh" tts:position="center" tts:displayAlign="center"/>
</layout>
</head>
</tt>
</config>
