<?xml version="1.0" encoding="UTF-8"?>
<!-- valid - tts:visibility attribute permitted with imsc11 text profile -->
<tt xml:lang="en"
    xmlns="http://www.w3.org/ns/ttml"
    xmlns:ttp="http://www.w3.org/ns/ttml#parameter" ttp:contentProfiles="http://www.w3.org/ns/ttml/profile/imsc1.1/text"
    xmlns:tts="http://www.w3.org/ns/ttml#styling" tts:extent="1280px 720px"
    xmlns:ttva="http://skynav.com/ns/ttv/annotations" ttva:warnOn="all" ttva:expectedErrors="0" ttva:expectedWarnings="0" ttva:model="imsc11">
  <head>
    <layout>
      <region xml:id="r1" tts:extent="640px 480px" tts:position="center" tts:visibility="visible">
        <style tts:visibility="visible"/>
        <style tts:visibility="hidden"/>
      </region>
    </layout>
  </head>
  <body region="r1" begin="0s" end="1s" tts:visibility="visible">
    <div tts:visibility="visible">
      <p tts:visibility="visible">
        <span tts:visibility="visible">Visibility</span>
      </p>
    </div>
  </body>
</tt>
