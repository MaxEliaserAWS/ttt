    xmlns:ttva="http://skynav.com/ns/ttv/annotations" ttva:warnOn="all" ttva:expectedErrors="0" ttva:expectedWarnings="0" ttva:model="imsc11">
  <head>
    <styling>
      <!-- visual style properties -->
      <initial tts:backgroundColor="black"/>
      <initial tts:border="1px solid red"/>
      <initial tts:color="green"/>
      <initial tts:direction="rtl"/>
      <initial tts:disparity="1px"/>
