<?xml version="1.0" encoding="utf-8"?>
<!-- valid - uses itts:forcedDisplay in image profile -->
<tt xml:lang="" xmlns="http://www.w3.org/ns/ttml"
    xmlns:ttm="http://www.w3.org/ns/ttml#metadata"
    xmlns:ttp="http://www.w3.org/ns/ttml#parameter" ttp:contentProfiles="http://www.w3.org/ns/ttml/profile/imsc1.1/image"
    xmlns:tts="http://www.w3.org/ns/ttml#styling" tts:extent="1280px 720px"
    xmlns:itts="http://www.w3.org/ns/ttml/profile/imsc1#styling"
    xmlns:ttva="http://skynav.com/ns/ttv/annotations" ttva:warnOn="all" ttva:expectedErrors="0" ttva:expectedWarnings="0" ttva:model="imsc11">
  <head>
    <layout>
      <region xml:id="r1" tts:extent="640px 120px" tts:origin="320px 528px" itts:forcedDisplay="true">
        <style itts:forcedDisplay="false"/>
        <style itts:forcedDisplay="true"/>
      </region>
    </layout>
  </head>
  <body region="r1" itts:forcedDisplay="true">
    <div itts:forcedDisplay="true">
      <metadata><ttm:item name="altText">test image</ttm:item></metadata>
      <image src="../../resources/images/outis.png" type="image/png" tts:extent="640px 120px"/>
    </div>
  </body>
</tt>
