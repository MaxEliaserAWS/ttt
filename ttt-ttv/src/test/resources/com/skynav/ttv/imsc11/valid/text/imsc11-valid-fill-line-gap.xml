<?xml version="1.0" encoding="UTF-8"?>
<!-- valid - itts:fillLineGap attribute permitted with imsc11 text profile -->
<tt xml:lang="en"
    xmlns="http://www.w3.org/ns/ttml"
    xmlns:ttp="http://www.w3.org/ns/ttml#parameter" ttp:contentProfiles="http://www.w3.org/ns/ttml/profile/imsc1.1/text"
    xmlns:tts="http://www.w3.org/ns/ttml#styling" tts:extent="1280px 720px"
    xmlns:itts="http://www.w3.org/ns/ttml/profile/imsc1#styling"
    xmlns:ttva="http://skynav.com/ns/ttv/annotations" ttva:warnOn="all" ttva:expectedErrors="0" ttva:expectedWarnings="0" ttva:model="imsc11">
  <head>
    <layout>
      <region xml:id="r1" tts:extent="640px 480px" tts:position="center" itts:fillLineGap="true">
        <style itts:fillLineGap="false"/>
        <style itts:fillLineGap="true"/>
      </region>
    </layout>
  </head>
  <body region="r1" begin="0s" end="1s" itts:fillLineGap="true">
    <div itts:fillLineGap="true">
      <p itts:fillLineGap="true">
        <span>Fill Line Gap</span>
      </p>
    </div>
  </body>
</tt>
